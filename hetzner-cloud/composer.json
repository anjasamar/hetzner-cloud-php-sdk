{
  "name": "atsicorp/hetzner-cloud",
  "description": "A PHP SDK for the Hetzner Cloud API.",
  "type": "library",
  "keywords": [
    "hetzner",
    "hcloud",
    "hetzner cloud",
    "cloud php library"
  ],
  "require": {
    "php": "^7.1|^8.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "illuminate/collections": "^5.5|^v6.18|^7.0|^8.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.5.5|^9.0",
    "phpstan/phpstan": "^0.12.43"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Anjas Amar Pradana",
      "email": "atsidev.io@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "atsicorp\\HetznerCloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atsicorp\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "docs": "php phpDocumentor.phar -d ./src -t ./docs"
  }
}
